{
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "tabWidth": 2,
  "importOrder": ["<THIRD_PARTY_MODULES>", "^[@/]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
